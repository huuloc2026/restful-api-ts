{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": "./src",
    "paths": {
      "*": [
        "*",
        "dist/*"
      ]
    }
  },
  "include": [
    "src"
, "controller/User.controllers.ts"  ],
  "exclude": [
    "node_modules"
  ]
}